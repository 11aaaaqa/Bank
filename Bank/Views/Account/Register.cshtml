<link rel="stylesheet" href="~/css/Input.css" />
@model Bank.Models.Register
<div class="form-center">
    <form method="post" asp-controller="Account" asp-action="Register">
        <div asp-validation-summary="ModelOnly"></div>
        <div>
            <label asp-for="TelephoneNumber"></label><br />
            <input asp-for="TelephoneNumber" />
            <span asp-validation-for="TelephoneNumber"></span>
        </div>
        <div>
            <label asp-for="Email"></label><br />
            <input asp-for="Email" />
            <span asp-validation-for="Email"></span>
        </div>
        <label asp-for="DateOfBirth"></label><br />
        <input asp-for="DateOfBirth" />
        <span asp-validation-for="DateOfBirth"></span>
        <div>
            <label asp-for="Password"></label><br />
            <input asp-for="Password" />
            <span asp-validation-for="Password"></span>
        </div>
        <div>
            <label asp-for="PasswordConfirm"></label><br />
            <input asp-for="PasswordConfirm" />
            <span asp-validation-for="PasswordConfirm"></span>
        </div>
        <p></p>
        <div>
            <input type="submit" value="Зарегистрироваться" />
        </div>
    </form>
</div>